<div class="row">
  <div class="col-md-3">
    <%= render 'layouts/master_sidemenu' %>
  </div>
  <div class="col-md-9">
    <h1>クール一覧</h1>
    <table class="table">
      <tr>
        <th>Year</th>
        <th>Season</th>
      </tr>
      <% @cours.each do |cour| %>
        <tr>
          <td><%= cour.year %></td>
          <td><%= cour.season %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>